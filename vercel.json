{
    "framework": null,
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/dist",
    "rewrites": [
        {
            "source": "/api/analyze",
            "destination": "/api/analyze"
        }
    ],
    "functions": {
        "api/analyze.py": {
            "runtime": "@vercel/python@4.6.0",
            "maxDuration": 60
        }
    }
}